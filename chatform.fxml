            <Label layoutX="17.0" layoutY="161.0" prefHeight="27.0" prefWidth="123.0" text="Users Online:" textFill="WHITE" />
            <Button id="exit" layoutX="15.0" layoutY="513.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="121.0" text="Exit">
               <font>
                  <Font name="Palatino Linotype" size="15.0" />
               </font>
            </Button>
         </children>
      </Pane>
    </children>
</AnchorPane>
